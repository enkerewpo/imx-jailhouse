/dts-v1/;

/ {
 model = "Jailhouse cell on ESPRESSObin";

 #address-cells = <2>;
 #size-cells = <2>;

 interrupt-parent = <&gic>;

 hypervisor {
  compatible = "jailhouse,cell";
 };

 cpus {
  #address-cells = <2>;
  #size-cells = <0>;

  cpu@1 {
   compatible = "arm,cortex-a53", "arm,armv8";
   device_type = "cpu";
   reg = <0x0 0x1>;
   enable-method = "psci";
  };
 };

 psci {
  compatible = "arm,psci-0.2";
  method = "smc";
 };

 timer {
  compatible = "arm,armv8-timer";
  interrupts = <1 13 4>,
        <1 14 4>,
        <1 11 4>,
        <1 10 4>;
 };

 gic: interrupt-controller@d1d00000 {
  compatible = "arm,gic-v3";
  #interrupt-cells = <3>;
  interrupt-controller;
  reg = <0x0 0xd1d00000 0x0 0x10000>,
        <0x0 0xd1d60000 0x0 0x20000>;
 };

 uart0: serial@d0012000 {
  compatible = "marvell,armada-3700-uart";
  reg = <0x0 0xd0012000 0x0 0x200>;
  interrupts = <0 11 4>,
        <0 12 4>,
        <0 13 4>;
  interrupt-names = "uart-sum", "uart-tx", "uart-rx";
 };

 pci@fc000000 {
  compatible = "pci-host-ecam-generic";
  device_type = "pci";
  bus-range = <0 0>;
  #address-cells = <3>;
  #size-cells = <2>;
  #interrupt-cells = <1>;
  interrupt-map-mask = <0 0 0 7>;
  interrupt-map = <0 0 0 1 &gic 0 108 1>,
    <0 0 0 2 &gic 0 109 1>,
    <0 0 0 3 &gic 0 110 1>,
    <0 0 0 4 &gic 0 111 1>;
  reg = <0x0 0xfc000000 0x0 0x100000>;
  ranges =
   <0x02000000 0x00 0x10000000 0x0 0x10000000 0x00 0x10000>;
 };
};
